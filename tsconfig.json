{
  "compilerOptions": {
    "target": "ES2023",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "noEmit": true,
  },
  "include": [
    "src"
  ]
}